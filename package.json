{
  "name": "react-style-prefixer",
  "version": "0.1.0",
  "description": "Autoprefixer for React Inline Styles using userAgent and caniuse data",
  "main": "lib/prefixer.js",
  "files": [
    "LICENSE",
    "README.md",
    "lib/",
    "dist/"
  ],
	"scripts" : {
    "transpile" : "babel src --out-dir lib && babel generator --out-dir dist",
		"pretest" : "npm run transpile && node dist/generatePrefixData.js",
    "test": "mocha --recursive --compilers js:babel/register",
		"prepublish" : "npm run transpile",
		"postinstall" : "node dist/generatePrefixData.js"
	},
  "repository": {
    "type": "git",
    "url": "https://github.com/rofrischmann/react-style-prefixer"
  },
  "keywords": [
    "react",
    "react styling",
    "prefixer",
    "inline styles",
    "autoprefixer",
    "vendor prefix",
    "userAgent"
  ],
  "author": "Robin Frischmann",
  "license": "MIT",
  "devDependencies": {
    "babel": "^5.6.14",
    "caniuse-api": "^1.3.2",
    "chai": "^3.2.0",
    "mocha": "^2.2.5"
  }
}
